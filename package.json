{
  "name": "projectname",
  "version": "0.0.1",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts --inspect=0.0.0.0:9229",
    "start:watch": "nodemon -L src/main.ts --inspect=0.0.0.0:9229 --exec ts-node",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|json)\"",
    "formatTest": "prettier --write \"tests/**/*.+(js|jsx|ts|tsx|json)\"",
    "test": "jest --watchAll",
    "test:coverage": "jest --watchAll --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watchAll",
    "test:ci": "jest --collect-coverage --watchAll=false --passWithNoTests",
    "publish:test:coverage": "codecov",
    "lint-staged": "lint-staged",
    "validate": "npm run format && npm run formatTest && npm run lint && npm run test:ci",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged"
  },
  "author": "juanjofp, francescorivola",
  "repository": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-filenames": "^1.3.2",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^13.1.2",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run format",
      "npm run test:ci",
      "npm run build"
    ]
  }
}